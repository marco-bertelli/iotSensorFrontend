    "marked": "^0.5.2",
    "nebular-icons": "1.1.0",
    "ng-inline-svg": "^11.0.0",
    "ng-lazyload-image": "^7.1.0",
    "ng2-ckeditor": "^1.2.9",
    "ng2-smart-table": "^1.6.0",
