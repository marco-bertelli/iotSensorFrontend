    "@nebular/security": "8.0.0",
    "@nebular/theme": "8.0.0",
    "@swimlane/ngx-charts": "^14.0.0",
    "angular2-chartjs": "0.4.1",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "bootstrap": "^4.4.1",
    "chart.js": "2.7.1",
    "ckeditor": "4.7.3",
    "classlist.js": "1.1.20150312",
    "colors.js": "1.2.4",
    "core-js": "2.5.1",
    "echarts": "^4.0.2",
    "eva-icons": "^1.1.3",
    "gulp-bump": "2.7.0",
